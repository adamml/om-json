{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://raw.githubusercontent.com/adamml/om-json/master/ocean-acidification/Reference.json",
	"title": "Reference",
	"description": "Describes the recommended citation text for a reference, and provides a link to the journal paper or methodology description",
	"type": "object",
	"properties": {
		"id": {
			"title": "id",
			"description": "A unique identifier for this reference",
			"type": "string"
		},
		"anyOf":[
			{"$ref","http://raw.githubusercontent.com/adamml/om-json/master/ocean-acidification/Common.json#/definitions/doiOrWebAddress"},
			{"$ref","http://raw.githubusercontent.com/adamml/om-json/master/ocean-acidification/Common.json#/definitions/citation"},
			{"$ref","http://raw.githubusercontent.com/adamml/om-json/master/ocean-acidification/Common.json#/definitions/informalReference"}
		]
	},
	"required": [ "id"],
	"additionalProperties": false
}
